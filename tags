RINGS	rings.py	/^RINGS = True$/;"	v
bounds	rings.py	/^bounds = set_bounds(limx, limy)$/;"	v
bounds	spirals-2.py	/^    bounds = get_bounds_from_sections(goldens)$/;"	v
bounds	spirals-2.py	/^bounds = set_bounds(limx, limy)$/;"	v
c	rings.py	/^        c = circle(A, pt, classes=['ring'])$/;"	v
cmap	spirals.py	/^cmap = mp.cm.get_cmap(cmap_name)$/;"	v
cmap_name	spirals.py	/^cmap_name = 'copper'$/;"	v
cycles	spirals.py	/^cycles = 13$/;"	v
d	rings.py	/^            d = float(pt.distance(test_pt))$/;"	v
distances	rings.py	/^        distances = []$/;"	v
gen	spirals.py	/^        gen = {'n': i, 'cmap': cmap, 'color_cycle': cycle} $/;"	v
generations	spirals.py	/^generations = []$/;"	v
nearest_pt	rings.py	/^        nearest_pt = pts[nearest_pt_id]$/;"	v
nearest_pt_id	rings.py	/^        nearest_pt_id = distances.index(min(distances)) + i + 1$/;"	v
nodes	spirals.py	/^nodes = 1086$/;"	v
num_rings	rings.py	/^num_rings = int(input(f'\\nnumber of rings: '))$/;"	v
phi	spirals.py	/^phi = (1 + math.sqrt(5)) \/ 2$/;"	v
pt	rings.py	/^        pt = pts[i]$/;"	v
pt	rings.py	/^    pt = point(rad, 0)$/;"	v
pt	rings.py	/^    pt = pt.rotate(i*sweep)$/;"	v
rad	rings.py	/^    rad = num_rings - i$/;"	v
rays	rings.py	/^rays = []$/;"	v
results	spirals.py	/^    results = pool.map(spiral_params, generations)$/;"	v
spiral	spirals.py	/^def spiral(n=144, cmap=mp.cm.YlGn, color_cycle=21, rev=False, offset=0):$/;"	f
spiral_params	spirals.py	/^def spiral_params(params):$/;"	f
start_time	rings.py	/^start_time = timer()$/;"	v
start_time	spirals-2.py	/^start_time = timer()$/;"	v
sweep	rings.py	/^sweep = 2 * sp.pi - (2 * sp.pi * sp.Rational(89, 144))$/;"	v
sweep	spirals.py	/^sweep = (2 * math.pi) - ((2 * math.pi) \/ phi)$/;"	v
theta	rings.py	/^theta = math.degrees(sweep)$/;"	v

ANALYZE	spirals-2.py	/^ANALYZE = True$/;"	v
BLUE	spirals-2.py	/^BLUE = True$/;"	v
BUILD	rings.py	/^BUILD = True$/;"	v
BUILD	spirals-2.py	/^BUILD = False$/;"	v
Ce	spirals-2.py	/^Ce = pts[3]$/;"	v

NAME	rings.py	/^NAME = f'rings-{num_rings}'$/;"	v
NAME	spirals-2.py	/^NAME = 'star-'$/;"	v
O	spirals-2.py	/^O = pts[6]$/;"	v
PART1	spirals-2.py	/^PART1 = False$/;"	v
